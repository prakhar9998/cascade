{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
}
